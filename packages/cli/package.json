{
  "name": "@warehouseoetzidev/cli",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build:linux": "bun build --target=bun --platform=linux --minify ./src/index.ts --compile --outfile=dist/linux/warehouse",
    "build:macos": "bun build --target=bun --platform=darwin --minify ./src/index.ts --compile --outfile=dist/darwin/warehouse",
    "build:windows": "bun build --target=bun --platform=windows --minify ./src/index.ts --compile --outfile=dist/windows/warehouse.exe"
  },
  "devDependencies": {},
  "dependencies": {
    "@warehouseoetzidev/core": "workspace:*",
    "@warehouseoetzidev/printer": "workspace:*",
    "@holmlibs/unzip": "1.0.0"
  }
}
