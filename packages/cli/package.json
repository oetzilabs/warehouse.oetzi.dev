{
  "name": "@warehouseoetzidev/cli",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build:linux": "bun build --platform=linux --minify --target=x86_64-unknown-linux-gnu ./src/index.ts",
    "build:macos": "bun build --platform=darwin --minify --target=x86_64-apple-darwin ./src/index.ts",
    "build:windows": "bun build --platform=windows --minify --target=x86_64-pc-windows-msvc ./src/index.ts"
  },
  "dependencies": {
    "@warehouseoetzidev/core": "workspace:*"
  }
}
